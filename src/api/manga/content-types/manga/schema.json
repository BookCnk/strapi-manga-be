{
  "kind": "collectionType",
  "collectionName": "mangas",
  "info": {
    "singularName": "manga",
    "pluralName": "mangas",
    "displayName": "manga",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "manga_status": {
      "type": "enumeration",
      "enum": [
        "END",
        "IN PROGRESS"
      ]
    },
    "author": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "views": {
      "type": "integer"
    },
    "likes": {
      "type": "integer"
    },
    "cover_image_url": {
      "type": "text"
    },
    "rating": {
      "type": "decimal"
    },
    "last_chapter_number": {
      "type": "integer"
    },
    "last_updated_at": {
      "type": "datetime"
    },
    "bookmarks": {
      "type": "integer"
    },
    "chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter.chapter"
    }
  }
}
